{
    "name": "npm-registry-download-count",
    "version": "1.0.0",
    "description": "An opinionated client for the npm package download counts API written in TypeScript",
    "scripts": {
        "init-environment": "husky install",
        "build": "tsup",
        "lint": "eslint --ext .ts,.cjs --no-error-on-unmatched-pattern .",
        "lint-fix": "eslint --fix --ext .ts,.cjs --no-error-on-unmatched-pattern .",
        "test": "vitest --run --coverage",
        "docs": "typedoc"
    },
    "type": "module",
    "keywords": [
        "npm",
        "ts",
        "downloads",
        "download",
        "registry",
        "api",
        "client",
        "count"
    ],
    "author": {
        "name": "Andrea Amorosi",
        "email": "dreamorosi@gmail.com"
    },
    "license": "MIT-0",
    "engines": {
        "node": ">=18"
    },
    "lint-staged": {
        "*.ts": "npm run lint-fix",
        "*.cjs": "npm run lint-fix"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json",
        "./README.md": "./README.md",
        "./LICENSE": "./LICENSE"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/node": "^20.8.0",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "@vitest/coverage-v8": "^0.34.6",
        "esbuild": "^0.19.4",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-node": "^0.3.9",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "tsup": "^7.2.0",
        "typedoc": "^0.25.1",
        "typedoc-plugin-missing-exports": "^2.1.0",
        "typescript": "^5.2.2",
        "vitest": "^0.34.6"
    },
    "dependencies": {
        "date-fns": "^2.30.0",
        "undici": "^5.25.2",
        "zod": "^3.22.2"
    }
}